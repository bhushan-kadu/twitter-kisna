{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "functions": {
        "controllers/*.js": { "maxDuration": 60 },
        "middleware/**/*.js": { "maxDuration": 60 },
        "api/*.js": { "maxDuration": 60 },
        "./index.js": { "maxDuration": 60 }
        
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }]
}
